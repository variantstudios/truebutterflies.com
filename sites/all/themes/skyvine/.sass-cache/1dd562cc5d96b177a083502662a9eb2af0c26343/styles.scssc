/*----- TEXT -----*/

h1, h2, h3, h4, h5, h5, h6 {
  font-family: 'Cabin', Helvetica, sans-serif;
  color: $headings;
  font-weight: normal;
  margin: 25px 0 10px;
  padding: 0 10px;
}
header h1 {background: url("../images/logo.png") no-repeat 50px 40px;}
h1 { font-size: 19px; }
h2 { font-size: 30px; }
h3 { font-size: 24px; font-family: 'Leckerli One', cursive; font-weight: normal; text-shadow: $text-shadow; }
h4 { font-size: 20px; }
h5 { font-size: 18px; }
h6 { font-size: 16px; font-weight: bold; text-transform: uppercase;}

h1 a:link, h1 a:visited,
h2 a:link, h2 a:visited,
h3 a:link, h3 a:visited,
h4 a:link, h4 a:visited,
h5 a:link, h5 a:visited,
h6 a:link, h6 a:visited {
  color: $headings;
  text-decoration: none;
}
h1 a:hover, h1 a:active,
h2 a:hover, h2 a:active,
h3 a:hover, h3 a:active,
h4 a:hover, h4 a:active,
h5 a:hover, h5 a:active,
h6 a:hover, h6 a:active {
  text-decoration: underline;
}
.visuallyhidden { border: 0; clip: rect(0 0 0 0); height: 1px; margin: -1px; overflow: hidden; padding: 0; position: absolute; width: 1px; }

p, div { color: $text; font: 14px/22px 'Cabin', sans-serif; }
p { margin: 0 10px 20px;}
a:link, a:visited, .nolink { color: $links; text-decoration: none; }
a:hover, a:active, .nolink:hover { text-decoration: none; color: #95C2A2;}


