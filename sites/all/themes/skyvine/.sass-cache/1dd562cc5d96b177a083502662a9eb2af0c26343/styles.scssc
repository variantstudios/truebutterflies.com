/* ------------- STYLES FOR RIBBON ON THE NAV --------------*/
nav {
  position: relative; }

nav:before {
  content: " ";
  display: block;
  width: 0;
  height: 0;
  border-top: 15px solid #23423b;
  border-left: 15px solid transparent;
  position: absolute;
  left: 0;
  top: 71px;
  margin-bottom: -10px; }

nav:after {
  content: " ";
  display: block;
  width: 0;
  height: 0;
  border-top: 15px solid #23423b;
  border-right: 15px solid transparent;
  position: absolute;
  left: 995px;
  top: 71px;
  margin-bottom: -10px; }
  
