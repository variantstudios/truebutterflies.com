/*---------------- MAIN NAVIGATION ---------------*/

	#main-nav {
		@include nav-bg;
		height: 70px;
		margin: 0 -15px;
		border-bottom: 1px solid #386d60;
		display: block;
		}
  	ul#main-menu {
  		margin: 0 30px 0 30px;
  		padding: 25px 0;
  		display: block;
    li {
    	font-family: $nav-font;
    	font-size: 24px;
    	font-weight: 700;
      	display: inline;
      	padding: 10px 38px;
      	border-right: 1px solid #418c6f;
  	} 
  	li:last-child {
  		border: none;
  	}
      a {
        &:link, &:visited {
          color: $main-nav;
          text-shadow: $text-shadow;
          text-decoration: none;        
         }   
        &.active {
           color: #abdbc5;
           text-shadow: 0px 2px 2px rgba(0, 0, 0, 0.45);
        }
        &:hover, &:active {
        text-decoration: none;
        }
      }
  }

 
      
/* ------------- STYLES FOR RIBBON ON THE NAV --------------*/
nav {
  position: relative; }

nav:before {
  content: " ";
  display: block;
  width: 0;
  height: 0;
  border-top: 15px solid #23423b;
  border-left: 15px solid transparent;
  position: absolute;
  left: 0;
  top: 71px;
  margin-bottom: -10px; }

nav:after {
  content: " ";
  display: block;
  width: 0;
  height: 0;
  border-top: 15px solid #23423b;
  border-right: 15px solid transparent;
  position: absolute;
  left: 995px;
  top: 71px;
  margin-bottom: -10px; }
  
